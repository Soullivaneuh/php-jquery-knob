{
    "name": "sllh/php-jquery-knob",
    "description": "PHP integration of aterrien/jQuery-Knob library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/options-resolver": "~2.7"
    },
    "require-dev": {
        "satooshi/php-coveralls": "~0.6",
        "codeclimate/php-test-reporter": "~0.2",
        "sllh/php-cs-fixer-styleci-bridge": "^1.3.3"
    },
    "autoload": {
        "psr-4": { "SLLH\\Knob\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "SLLH\\Knob\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
